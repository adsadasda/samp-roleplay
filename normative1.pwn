	else if(pData[playerid][food_have] == 1 && !IsPlayerInRangeOfPoint(playerid, 8.0, 1146.06201, -1755.98499, 15.11)) return SCM(playerid, COLOR_LIGHTGREY, "Нужно находиться около бизнеса");
