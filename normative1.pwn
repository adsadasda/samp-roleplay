	if(params[0] == playerid) return SCM(playerid, COLOR_GREY, "Вы не можете передать цветы себе");
	if(!ProxDetectorS(2.0, playerid, params[0])) return SCM(playerid, COLOR_GREY, "Игрок слишком далеко от Вас");
