stock PlayStream(playerid, url[], Float:posX = 0.0, Float:posY = 0.0, Float:posZ = 0.0, Float:distance = 50.0, usepos = 0) {
	if(GetPVarType(playerid, #pAudioStream)) StopAudioStreamForPlayer(playerid);
