	else if(GetPVarInt(playerid, "bbox_mtype") == 2) {
		for(new i = 0; i < 29; i ++) {
			if((i > 6 && i < 17) || (i > 26)) TextDrawHideForPlayer(playerid, rec_td[i]);
			else TextDrawShowForPlayer(playerid, rec_td[i]);
