		else mysql_format(mysql_Handle, string, 245, "INSERT INTO `logs` (`Text`, `id_acc`, `Date`) VALUES ('Администратор %s забанила игрока %s na %d дней. Причина: %s',%d,'%s')", pData[playerid][pName], GetName(params[0]), params[1], params[2], pData[params[0]][pID], data2);
		mysql_tquery(mysql_Handle, string);
