    }
    if(pData[playerid][autoHave] != 0) {
    	SCMF(to_player, COLOR_BLUE, "%s хочет продать Вам свой бизнес "CYELLOW"Автомобильная фабрика "CBLUE"за %d$", pData[playerid][pName],price);
