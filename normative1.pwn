stock withdraw_donate(playerid, cena, name[]) {
	pData[playerid][pRub] -= cena;
	UpdatePlayerData(playerid, "pRub", pData[playerid][pRub]);
	new msg[164], game_strr[26], years, months, days, data[32];
	getdate(years, months, days);
	format(game_strr, sizeof game_strr, "~r~-%d ~w~rub", cena);
	GameTextForPlayer(playerid, game_strr, 3000, 1);
	format(data, 32, "%02d.%02d.%04d", days, months, years);
	mysql_format(mysql_Handle, msg, 164, "INSERT INTO `donate_log` (`NamePlayer`, `Price`, `Name`, `Date`) VALUES ('%s', %d, '%s', '%s')", pData[playerid][pName],cena,name,data);
	return mysql_tquery(mysql_Handle, msg);
}
stock mafia_menu(playerid) {
	if(IsPlayerInRangeOfPoint(playerid, 3.0, 2606.9250,1303.7983,1051.6802)) {
		if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return 1;
		new vw_player = GetPlayerVirtualWorld(playerid);
		if(vw_player != 2 && vw_player != 5 && vw_player != 3) return 1;
		if(vw_player == 2 && pData[playerid][pMember] != 5) return SCM(playerid, COLOR_LIGHTGREY, "Nie należysz do La Cosa Nostra");
		else if(vw_player == 5 && pData[playerid][pMember] != 14) return SCM(playerid, COLOR_LIGHTGREY, "Nie należysz do rosyjskiej mafii");
		else if(vw_player == 3 && pData[playerid][pMember] != 6) return SCM(playerid, COLOR_LIGHTGREY, "Nie należysz do Yakuza");
		SPD(playerid, 7171, DIALOG_STYLE_LIST,"{ffcc15}Magazyn mafii", 
			"[1] Włóż amunicję\n\
			[2] Włóż dragi\n\
			[3] Włóż metal\n\
			{99cc66}[4] Weź amunicję\n\
			{99cc66}[5] Weź dragi\n\
			{99cc66}[6] Weź metal\n\
			"CWHITE"[7] Weź bombę", 
		"Dalej", "Anuluj");
	}
	return 1;
}
stock GangMenu(playerid) {
	if(IsPlayerInRangeOfPoint(playerid, 1.75, 2740.0842,-1957.2704,662.6328)) {
		if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return 1;

		new vw=GetPlayerVirtualWorld(playerid),
			org=pData[playerid][pMember];
		if(vw==34 && org !=12) 
			return SCM(playerid, COLOR_LIGHTGREY, "Nie należysz do The Ballas");
		else if(vw==75 && org !=13) 
			return SCM(playerid, COLOR_LIGHTGREY, "Nie należysz do Los Santos Vagos");
		else if(vw==1 && org !=15) 
			return SCM(playerid, COLOR_LIGHTGREY, "Nie należysz do Grove Street");
		else if(vw==36 && org !=17) 
			return SCM(playerid, COLOR_LIGHTGREY, "Nie należysz do Varrios Los Aztecas");
		else if(vw==63 && org !=18) 
			return SCM(playerid, COLOR_LIGHTGREY, "Nie należysz do The Rifa");

		switch(org) {
			case 12: if(b_Sklad{0} == true) return SCM(playerid, COLOR_ERROR, "Magazyn zamknięty");
			case 13: if(b_Sklad{1} == true) return SCM(playerid, COLOR_ERROR, "Magazyn zamknięty");
			case 15: if(b_Sklad{2} == true) return SCM(playerid, COLOR_ERROR, "Magazyn zamknięty");
			case 17: if(b_Sklad{3} == true) return SCM(playerid, COLOR_ERROR, "Magazyn zamknięty");
			case 18: if(b_Sklad{4} == true) return SCM(playerid, COLOR_ERROR, "Magazyn zamknięty");
		}
		SPD(playerid, 7171, DIALOG_STYLE_LIST,"{ffcc15}Magazyn gangu", 
			"[1] Włóż amunicję\n\
			[2] Włóż dragi\n\
			[3] Włóż metal\n\
			{99cc66}[4] Weź amunicję\n\
			{99cc66}[5] Weź dragi\n\
			{99cc66}[6] Weź metal", 
		"Dalej", "Anuluj");
	}
	return 1;
}

