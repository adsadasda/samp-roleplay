	SetPVarInt(playerid, "BBLabel", _:CreateDynamic3DTextLabel(string, 0x66CC00FF, BBCoord[0], BBCoord[1], BBCoord[2]+0.6, 8, .worldid = GetPlayerVirtualWorld(playerid), .interiorid = GetPlayerInterior(playerid)));
