			SPD(playerid, 1303, DIALOG_STYLE_MSGBOX, "{ffffad}Квартира", fl_str, "Menu", "Anuluj");
		}
		else if(flat_info[i][fl_owned] && !flat_info[i][fl_rentstatus]) {
			format(fl_str, sizeof(fl_str), ""CWHITE"Номер mieszkania: "CMAIN"%d\n\n"CWHITE"Класс: "CMAIN"%d комнаты\n"CWHITE"Владелец: "CMAIN"%s\n"CWHITE"Гос. цена: "CMAIN"%d$", i, flat_info[i][fl_rooms], flat_info[i][fl_owner], flat_info[i][fl_price]);
			SPD(playerid, 1305, DIALOG_STYLE_MSGBOX, "{ffffad}Квартира", fl_str, "Wejdź", "Anuluj");
		}
		else {
			format(fl_str, sizeof(fl_str), ""CWHITE"Номер mieszkania: "CMAIN"%d\n\n"CWHITE"Класс: "CMAIN"%d комнаты\n"CWHITE"Цена: "CMAIN"%d$", i, flat_info[i][fl_rooms], flat_info[i][fl_price]);
			SPD(playerid, 1306, DIALOG_STYLE_MSGBOX, "{ffffad}Квартира", fl_str, "Купить", "Anuluj");
		}
	}

	for(new i = 1; i != 13; i ++) { 
		if(!IsPlayerInRangeOfPoint(playerid, 2, garage[i][grposition][0],garage[i][grposition][1],garage[i][grposition][2])) continue;
