stock ConvertNewLines(str[], texttoconv[], size = sizeof(str))
{
	format(str, size, texttoconv);
	for(new i = 0; i < strlen(str); i ++)
	{
		new find = strfind(str, "/");
		if(find != -1) strdel(str, find, find+1), strins(str, "\n", find, strlen(str));
	}
}

