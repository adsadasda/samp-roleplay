					ProxDetector(30.0, playerid, string, COLOR_PURPLE);
					break;
				}
			}
		}
		if(!next) return SCM(playerid, COLOR_LIGHTGREY, "В машине нет свободных мест");
	}
	else return SCM(playerid, COLOR_ERROR, "Wystąpił błąd [068]");
	return 1;
}
CMD:blind(playerid,params[])
