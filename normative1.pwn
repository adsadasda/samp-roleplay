CMD:e(playerid) {
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SCM(playerid, COLOR_LIGHTGREY, "Musisz siedzieć za kierownicą samochodu");
	new vehicleid = GetPlayerVehicleID(playerid);
	new fixed_veh = vehicleid - 1;
	if(fixed_veh <= -1 || fixed_veh >= MAX_VEHICLES) return SCM(playerid, COLOR_ERROR, "Wystąpił błąd [077]");
	if(vehicleid >= veh_taxi[0] && vehicleid <= veh_taxi[1] && !arenda_taxi[playerid]) return SCM(playerid, COLOR_GREY, "Nie można włączyć ani zgasić silnika w tym momencie");
	if(vehicleid >= veh_bus[0] && vehicleid <= veh_bus[1] && !arenda_bus[playerid]) return SCM(playerid, COLOR_GREY, "Nie można włączyć ani zgasić silnika w tym momencie");
	if(GetVehicleModel(vehicleid) == 481 || GetVehicleModel(vehicleid) == 509 || GetVehicleModel(vehicleid) == 510) return 1;
	if(VehInfo[vehicleid][vFuel] < 0.1 && !VehInfo[fixed_veh][vEngine]) {
		PlayerPlaySound(playerid, 25603, 0.0, 0.0, 0.0);
		return GameTextForPlayer(playerid, "~r~brak paliwa", 3000, 1);
	}
	new engine, lights, alarm, doors, bonnet, boot, objective;
	GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
    anti_flood_eng[playerid] = gettime() + 1;
    if(!VehInfo[fixed_veh][vEngine]) SetVehicleParamsEx(vehicleid, 1, lights, alarm, doors, bonnet, boot, objective);
    else SetVehicleParamsEx(vehicleid, 0, lights, alarm, doors, bonnet, boot, objective);
	return 1;
}
