		frac_gangzone[GZInfo[i][gFrakVlad]]++;
	}
	UpdateStoreGang(12); UpdateStoreGang(13); UpdateStoreGang(15); UpdateStoreGang(17); UpdateStoreGang(18);
	printf("%d зон банд na карте загружено | %d мс", rows, GetTickCount() - time);

