    }
    SetPVarInt(playerid, "cancel_act", 1); SetPVarInt(playerid, "cancel_act_id", to_player);
    return SCM(to_player, -1, "Нажмите "CMAIN"Y "CWHITE"чтобы согласиться или "CERROR"N "CWHITE"для отказа");
