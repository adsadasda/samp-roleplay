		UpdateDynamic3DTextLabelText(metarmy_3d[0], COLOR_LITEBLUE, "Доставка metalа\n"CORANGE"Загружено: "CERROR"0 из 700");
	}
	if(vehicleid == vvcveh[1] - 1) {
		metarmy_count[1] = 0;
		UpdateDynamic3DTextLabelText(metarmy_3d[1], COLOR_LITEBLUE, "Доставка metalа\n"CORANGE"Загружено: "CERROR"0 из 700");
	}
