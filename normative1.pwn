				case 1: obj = 1215;
				case 2: obj = 19122;
				case 3: obj = 19123;
				case 4: obj = 19124;
				case 5: obj = 19125;
				case 6: obj = 19126;
				case 7: obj = 19127;
			}
			blow_object[playerid][0] = CreateObject(obj, x, y, z-0.5, 0, 0, 0);
