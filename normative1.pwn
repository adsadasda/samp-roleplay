	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return 1;
	Hook_GivePlayerMoney(playerid, -500);
	TogglePlayerControllable(playerid, 1);
	new car = GetPlayerVehicleID(playerid);
