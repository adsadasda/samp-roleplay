stock SplitValue(value, str[], size = sizeof(str))
{
	format(str, size, "%i", value < 0 ? -value : value);
	
	for(new i = strlen(str); (i -= 3) > 0;)
	{
		if(str[i] != EOS && '0' <= str[i] <= '9') strins(str, ".", i, size);
		else return;
	}
    if(value < 0) strins(str, "-", 0, size);
    return;
}

